{
  "expo": {
    "name": "PlantTime",
    "slug": "TestProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconPlant.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier" : "com.heretic.TestProject",
      "buildNumber" : "1.0.0"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.heretic.TestProject",
      "versionCode" : 1,
      "intentFilters": [
        {
        "action": "VIEW",
        "data": [
            {
            "scheme": "https",
            "host": "planttime-87863.firebaseapp.com",
            "pathPrefix": "/"
            }
        ],
        "category": ["BROWSABLE", "DEFAULT"]
        }
    ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "expo-image-picker"
    ],
    "extra": {
      "eas": {
        "projectId": "8c73983a-a1b5-4ca1-bc7e-78b3448b6a92"
      }
    }
  }
}
